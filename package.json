    "next": "14.2.3",
    "postcss": "8.4.38",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "tailwindcss": "3.4.3",
    "typescript": "5.4.5"
  },
  "devDependencies": {
    "@biomejs/biome": "1.7.3",
    "@types/node": "20.12.12",
    "@types/react": "18.3.2",
    "@types/react-dom": "18.3.0",
    "encoding": "0.1.13",
    "npm-run-all": "4.1.5",
    "prettier": "3.2.5",
    "supabase": "1.169.8"
  }
}
